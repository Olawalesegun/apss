<!-- Add new survey form -->


        <form class=" mt-3 ">
            <!-- 2 column puts for the National number and Phome Number-->
            <div class="center d-flex justify-content-center mb-3 ">
                <h3 class=" ">
                    Survey
                </h3>
            </div>
            <!-- survey Name -->
            <div class="form-outline mb-4 ">
                <input type="tel" id="Survey_Name" name="Survey_Name" class="form-control" />
                <label class="form-label" for="Survey_Name">Survey Name</label>
            </div>
            <!-- post and end survey date -->
            <div class="row mb-4">
                <div class="col">
                    <div class="form-outline datepicker input-group.date">
                        <input type="date" id="sandbox-container " name="post_date" class="form-control" />
                        <label class="form-label" for="post_date">Post Date</label>
                    </div>
                </div>
                <div class="col">
                    <div class="form-outline datepicker input-group.date-p mb-2">
                        <input type="date" id="sandbox-container-p" name="end_date" class="form-control" />
                        <label class="form-label" for="end_date">End Date</label>
                    </div>
                </div>
            </div>

            <!-- Text input survey description-->
            <div class="form-outline mb-4">
                <textarea type="text" id="eduction_level" name="eduction_level" class="form-control"></textarea>
                <label class="form-label" for="eduction_level">Description</label>
            </div>

            <!-- specify survey post place-->
            <div class="form-outline  ">
                <h6 class="lh-1 border-bottom border-dark w-25 pb-1">Target Plase</h6>
            </div>
            <div class="row">
                <div class="col mb-4">
                    <div class="">
                        <select class="form-select " aria-label="Default select ">
                            <option selected>select City</option>
                            <option value="1">One</option>
                            <option value="2">Two</option>
                            <option value="3">Three</option>
                        </select>
                    </div>
                </div>
                <div class="col">
                    <div class="">
                        <select class="form-select " aria-label="Default select example">
                            <option selected>select </option>
                            <option value="1">One</option>
                            <option value="2">Two</option>
                            <option value="3">Three</option>
                        </select>
                    </div>
                </div>
            </div>
            <div class="form-select-outline mb-4">
                <select class="form-select  form-outline" aria-label="Default select example">
                    <option selected>select </option>
                    <option value="1">One</option>
                    <option value="2">Two</option>
                    <option value="3">Three</option>
                </select>
            </div>
            <!-- Submit button -->
            <button type="submit" class="btn btn-primary btn-block mb-4 mt-4">Add</button>
        </form>
